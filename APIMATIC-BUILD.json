{
    "$schema": "https://titan.apimatic.io/api/build/schema",
    "buildFileVersion": "1",
    "generatePortal": {
        "debug": {
            "publishReport": true
        },
        "pageTitle": "My Portal Title",
        "navTitle": "My Navigation Title",
        "logoUrl": "static/images/logo.png",
        "logoLink": "https://www.apimatic.io",
        "languageConfig": {
            "http": {},
            "java": {
                "packageRepository": "Maven",
                "packageSettings": {
                    "GroupId": "com.apimatic",
                    "PackageName": "sample-sdk-artifact-id",
                    "Version": "1.0.0"
                }
            },
            "csharp": {},
            "typescript": {},
            "php": {},
            "python": {},
            "ruby": {}
        },
        "headIncludes": "  <link href=\"https:\\/\\/fonts.googleapis.com\\/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i&display=swap\" rel=\"stylesheet\">",
        "portalSettings": {
            "theme": {
                "baseTheme": "standard",
                "colors": {
                    "primaryColor": {
                        "light": "#0C7FF2",
                        "dark": "#7CBAF8"
                    },
                    "linkColor": {
                        "light": "#004AD7",
                        "dark": "#A3D1FF"
                    }
                },
                "colorMode": {
                    "defaultMode": "system",
                    "disableSwitch": false
                }
            },
            "enableExport": true,
            "enableConsoleCalls": true
        },
        "tailIncludes": "<script defer src='./static/scripts/guided-walkthroughs/walkthrough-1.js'></script><script>document.addEventListener('DOMContentLoaded', (event) => {APIMaticDevPortal.ready(({ registerWorkflow }) => {registerWorkflow('page:calculate-api/guided-walkthroughs/walkthrough-1', 'Sample Workflow', SampleWorkflow);});});</script>"
    }
}
